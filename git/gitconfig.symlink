[user]
    name = Cameron Wardzala
    email = cwardzala@fastmail.com
    signingkey = 842A0F02F354309E
[credential]
    helper = osxkeychain
[color]
    diff = auto
    status = auto
    branch = auto
    ui = true
    interactive = true
[core]
    excludesfile = /Users/cwardzala/.gitignore
    editor = vim
    ignorecase = false
    symlinks = false
[apply]
    whitespace = nowarn
[mergetool]
    keepBackup = false
[help]
    autocorrect = 1
[push]
    default = simple
    followTags = true
[filter "media"]
    clean = git-media-clean %f
    smudge = git-media-smudge %f
[diff]
    tool = icdiff
[difftool]
    prompt = false
[difftool "icdiff"]
    cmd = /usr/local/bin/icdiff --line-numbers $LOCAL $REMOTE
[filter "lfs"]
    required = true
    clean = git-lfs clean %f
    smudge = git-lfs smudge %f
[color "diff"]
    old = red strike
    new = green italic
[commit]
    gpgsign = true
[alias]
    up = pull --rebase --autostash
    prune = fetch --prune
    undo = reset --soft HEAD^
    stash-all = stash save --include-untracked
    leaderboard = shortlog -sn
[gpg]
    program = /usr/local/bin/gpg
[status]
    showUntrackedFiles = all